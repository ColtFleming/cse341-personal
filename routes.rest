PUT http://localhost:3000/rosters/63325f752e2406fe7c0e841a HTTP/1.1
content-type: application/json

{
        "firstName": "Lindsay",
        "lastName": "Fleming",
        "email": "lindsayflemingisthebest@gmail.com",
        "favoriteColor": "Teal",
        "birthday": "12/11/87"
